<tr>
  <td>
    <%= image_tag product.image.tiny %>
  </td>
  <td>
    <h4><%= product.name %></h4>
    <br>
    <p><%= product.description %></p>
  </td>
  <td>
    <%= humanized_money_with_symbol item_price %>
    <% product.inspect %>
  </td>
  <td>
  <%= quantity %>
</td>
  <td>
    <%= humanized_money_with_symbol total_price %>
  </td>
</tr>
